@using System.Diagnostics
@using System.Net.Http
@{
    ViewData["Title"] = "Cart";
}
@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor

<div xmlns="http://www.w3.org/1999/html">
    <ul>
    
    @foreach (var i in ViewBag.products) {
        <li>
            @i
        </li>
    }
    
    </ul>
</div>